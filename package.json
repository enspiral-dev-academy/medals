{
  "name": "medals",
  "description": "Enspiral Dev Academy's Learning Management System",
  "version": "3.0.1",
  "homepage": "https://github.com/enspiral-dev-academy/medals",
  "author": "EDA",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/enspiral-dev-academy/medals.git"
  },
  "scripts": {
    "assert": "exit 0",
    "build": "webpack --config ./client/webpack.config.js",
    "dev": "nodemon --watch server & yarn run build --watch",
    "e2e:sh": "cd ./test/e2e && codeceptjs shell",
    "e2e:debug": "codeceptjs run -c ./test/e2e/codecept.json --debug",
    "e2e": "codeceptjs run -c ./test/e2e/codecept.json",
    "e2e:assert": "yarn run e2e && yarn run assert",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "seed": "yarn run knex seed:run",
    "lint": "eslint client server test --ignore-pattern 'bundle.js'",
    "lint:assert": "yarn run lint && yarn run assert",
    "postinstall": "yarn run build && npm run knex migrate:latest",
    "test": "jest test/client test/server",
    "test:watch": "yarn test --watch",
    "test:assert": "yarn test && yarn run assert",
    "start": "node server"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./test/client-setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "precommit": "lint:assert, test:assert, e2e:assert",
  "dependencies": {
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "knex": "^0.14.2",
    "moment": "^2.18.1",
    "pg": "^7.2.0",
    "sodium": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bulma": "^0.6.0",
    "codeceptjs-nightmare": "^1.1.0",
    "core-js": "^2.5.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "input-moment": "^0.4.0",
    "jest": "^21.1.0",
    "jsdom": "^11.2.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.5.10",
    "purecss": "^1.0.0",
    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "react": "^16.1.1",
    "react-addons-test-utils": "^16.0.0-alpha.3",
    "react-dom": "^16.1.1",
    "react-modal": "^3.1.4",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^16.1.1",
    "redux": "^3.5.2",
    "redux-mock-store": "^1.3.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^6.0.6",
    "sqlite3": "^3.1.9",
    "style-loader": "^0.19.0",
    "superagent": "^3.8.1",
    "supertest": "^3.0.0",
    "tachyons": "^4.8.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.4.5"
  }
}
